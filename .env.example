# 默认使用本地 Ollama（不需要 key）
LLM_PROVIDER=local

# 本地 Ollama
OLLAMA_BASE_URL=http://127.0.0.1:11434
OLLAMA_MODEL=deepseek-r1:8b
OLLAMA_TEMPERATURE=0.7
OLLAMA_TIMEOUT=60

# 云端 DeepSeek（阿里云百炼 DashScope - OpenAI 兼容模式）
# 获取 Key 后填到这里（不要提交真实 key）
DASHSCOPE_API_KEY=
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
# 按你的需求固定为 deepseek-v3.2（也可按需改）
DASHSCOPE_MODEL=deepseek-v3.2
DASHSCOPE_TEMPERATURE=0.7
DASHSCOPE_TIMEOUT=60

# 仅对云端 deepseek 生效：默认是否开启思考模式（enable_thinking）
DEEPSEEK_ENABLE_THINKING=true

